        flex: 0 0 auto;
        overflow: hidden;
        :global(.page-section-inner) {
            position: relative;
            margin-block-start: 64px;
        }

        h1, p {
            color: var(--background-primary);
            max-inline-size: min(50vw, 650px);
            @media only screen and (max-width: 905px) {
                max-inline-size: unset;
            }
        }
        
        &::after {
            content: "";
            position: absolute;
            inset-inline-start: 0;
            inset-block-start: -64px;
            inline-size: 100%;
            block-size: calc(100% - 64px);
            background-color: hsl(var(--base-accent));
            z-index: -2;
            clip-path: polygon(0 0, 100% 0, 100% 90%, 0% 100%);
        }
    }

    .hero-buttons {
        display: flex;
        align-items: center;
        flex-wrap: wrap;
        gap: 8px;
        :global(.button) {
            svg {
                inline-size: 20px;
                block-size: 20px;
                margin-right: -8px;
                margin-left: -8px;
                fill: currentColor;
            }
            &:global(.variant-filled) {
                color: var(--foreground-secondary);
                background-color: var(--background-secondary);
                &:hover {
                    background-color: var(--background-tertiary);
                }

            }
            &:global(.variant-outlined) {
                box-shadow: inset 0 0 0 1px var(--background-primary);
                color: var(--background-tertiary);
                &:hover, &:focus {
                    color: var(--foreground-secondary);
                    background-color: var(--background-secondary);
                }
                &:focus-visible {
